
<div class="row">
  <div class="span4">

<%= simple_form_for(resource, :as => resource_name, html: {class: "well"}, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <fieldset>

  <legend>Sign Up</legend>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :profile_name %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>

  <div><%= f.button :submit, "Sign in" %></div>
</fieldset>

<% end %>

<%= render "devise/shared/links" %>

</div><!-- /span4 -->

</div><!-- /row -->
